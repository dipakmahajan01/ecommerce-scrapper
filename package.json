{
  "name": "ecommerce-scrapper",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "npx rimraf build",
    "start": "node build/server.js",
    "dev": "nodemon --ignore '*.json' src/server.ts",
    "ts": "tsnd --respawn src/server.ts",
    "build": "tsc",
    "clean": "npx rimraf ./build",
    "dist": "sh ./scripts/build.sh",
    "server": "tsc-watch --outDir ./build --onSuccess \"npm run start -- --mode=http\" --onFailure \"echo ERROR! Compilation Failed\"",
    "debug": "tsc-watch --outDir ./build --onSuccess \"npm run runDebug -- --mode=http \" --onFailure \"echo ERROR! Compilation Failed\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall-production": "echo \"production, skipping...\"",
    "postinstall-dev": "git config core.hooksPath ./git-hooks",
    "postinstall": "if-env NODE_ENV=production && npm run postinstall-production || npm run postinstall-dev || true",
    "lint": "eslint .",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "populate:devices-list": "ts-node scripts/populateDeviceList.ts",
    "analyze:failures": "ts-node __test__/analyzeProcessorFailures.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "ai": "^5.0.117",
    "cors": "^2.8.5",
    "crawlee": "^3.15.3",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.20.0",
    "openai": "^6.9.1",
    "playwright": "^1.56.1",
    "puppeteer": "^24.31.0",
    "tsc-watch": "^7.2.0",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  }
}
